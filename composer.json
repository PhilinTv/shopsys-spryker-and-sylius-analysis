{
    "name": "tomasvotruba/shopsys-analysis",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-gmp": "*",
        "ext-bz2": "*",
        "phploc/phploc": "^4.0",
        "sebastian/phpcpd": "^3.0",
        "symfony/console": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symplify/package-builder": "^4.7",
        "symplify/easy-coding-standard": "^4.7",
        "symplify/coding-standard": "^4.7",
        "phpstan/phpstan-shim": "^0.10.3",
        "symplify/phpstan-extensions": "@dev",
        "nette/robot-loader": "^3.1",
        "nette/utils": "^2.5"
    },
    "require-dev": {
        "tracy/tracy": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\ShopsysAnalysis\\": "src"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check bin src",
        "fix-cs": "vendor/bin/ecs check bin src --fix",
        "phpstan": "vendor/bin/phpstan analyse bin src --level max"
    }
}
